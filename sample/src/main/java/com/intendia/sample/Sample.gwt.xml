<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.8.2//EN" "http://www.gwtproject.org/doctype/2.8.2/gwt-module.dtd">
<module rename-to="app">
    <inherits name="com.google.gwt.core.Core"/>
    <inherits name="com.google.gwt.resources.Resources"/>
    <inherits name="com.google.web.bindery.event.Event"/>
    <inherits name="com.intendia.mapboxgl.draw.MapboxDraw"/>
    <inherits name="com.intendia.mapboxgl.map.MapboxGL"/>
    <inherits name="org.jboss.gwt.elemento.Core"/>

    <entry-point class="com.intendia.sample.client.SampleEntryPoint"/>

    <set-configuration-property name="CssResource.enableGss" value="true" />
    <set-configuration-property name="CssResource.conversionMode" value="off" />
    <collapse-all-properties/>

    <source path="client"/>
    <public path="public"/>
</module>
